<Application x:Class="SpiceSharpGUI.Windows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:SpiceSharpGUI.Windows.ViewModels"
             xmlns:v="clr-namespace:SpiceSharpGUI.Windows.Views"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:NetlistWindowViewModel}">
            <v:NetlistView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:NetlistResultWindowViewModel}">
            <v:NetlistResultView />
        </DataTemplate>
    </Application.Resources>
</Application>
