<UserControl x:Class="SpiceSharpRunner.Windows.Views.NetlistView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpiceSharpRunner.Windows"
        mc:Ignorable="d"
        d:DesignHeight="600" d:DesignWidth="600" MinWidth="600">
        <DockPanel Height="Auto" Name="stackPanel1" Width="Auto" LastChildFill="True">
                <avalonEdit:TextEditor
                            xmlns:avalonEdit="http://icsharpcode.net/sharpdevelop/avalonedit"
                            Name="txtNetlist"
                            IsReadOnly="false"
                            ShowLineNumbers="True"
                            FontFamily="Consolas"
                            FontSize="10pt"/>
            </DockPanel>
</UserControl>
